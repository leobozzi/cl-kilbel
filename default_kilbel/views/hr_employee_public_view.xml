<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Hr Employee Public Form view -->
    <record model="ir.ui.view" id="custom_hr_employee_public_form_view">
        <field name="name">customt_hr_employee_public_form_view</field>
        <field name="model">hr.employee.public</field>
        <field name="inherit_id" ref="hr.hr_employee_public_view_form" />
        <field name="arch" type="xml">
            <field name="work_location_id" position="after">
                <field name="file_number" />
                <field name="cashier_number" />
            </field>
        </field>
    </record>
</odoo>
    